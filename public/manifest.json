{
  "name": "Togly",
  "description": "A clean and modern look to your default Chrome homepage. A Chrome dashboard.",
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": ["https://newsapi.org/*"],
  "action": {
    "default_icon": { "16": "logo.png", "24": "logo.png", "32": "logo.png" }
